---
import MainLayout from "@/layouts/MainLayout.astro";
import { SITE } from "@data/constants";
import PrimaryCTA from "@components/ui/buttons/PrimaryCTA.astro";

const pageTitle: string = `unv | ${SITE.Title + " - " + "Lovosis technology U.A.E"}`;
interface Props {
  title: string;
  subTitle?: string;
  benefits?: Array<string>;
}

const structuredData={
    "@context": "https://schema.org",
    "@type": "WebPage",
    "@id": "https://lovosis.com/dahua",
    "url": "https://lovosis.com/dahua",
    "name": "unv | Lovosis",
    "description": "Discover Lovosis, Dubai's authorized supplier of top-quality Surveillance & IT Solution. Learn about our commitment to excellence in the UAE construction industry.",
    "isPartOf": {
      "@type": "WebSite",
      "url": "https://lovosis.com",
      "name": "Lovosis",
      "description": "Custom Fabrication & Surveillance Solution Provider In Dubai, United Arab Emirates."
    },
    "inLanguage": "en-US"
  }
---

<style>
  .autoRotate{
    animation: autoRotateAnimation;
    animation-timeline: view();
  }
  @keyframes autoRotateAnimation{
      from{
          transform: rotate(0deg);
      }
      to{
          transform: rotate(360deg);
      }
  }  

  .autoShow{
    animation: autoShowAnimation both;
    animation-timeline: view(70% 5%);

    }

    @keyframes autoShowAnimation{
    from{
        opacity: 0;
        transform: translateY(200px) scale(0.3);
    }to{
        opacity: 1;
        transform: translateY(0) scale(1);
    }
    }

    .fadeInRight {
    animation: fadeInRightAnimation;
    animation-timeline: view();
    }

    @keyframes fadeInRightAnimation {
        from {
            opacity: 0;
            transform: translateX(100px);
        }
        to {
            opacity: 1;
            transform: translateX(0);
        }
    }

    .fadeInUp {
    animation: fadeInUpAnimation;
    animation-timeline: view();
    }

    @keyframes fadeInUpAnimation {
        from {
            opacity: 0;
            transform: translateX(100px);
        }
        to {
            opacity: 1;
            transform: translateX(0);
        }
    }

    .fadeInUps {
    animation: fadeInUpsAnimation;
    animation-timeline: view();
    }

    @keyframes fadeInUpsAnimation {
        from {
            opacity: 0;
            transform: translateY(100px);
        }
        to {
            opacity: 1;
            transform: translateY(0);
        }
    }


</style>


<MainLayout
  title={pageTitle} sd={structuredData}
 
>



<section class="mx-auto max-w-[85rem] px-4 py-10 sm:px-6 lg:px-8 lg:py-14 2xl:max-w-full">
<div class="grid lg:grid-cols-2 justify-items-center gap-5">
<div class="flex flex-col justify-center">
<img src="/pictures/Brand Profile/6.jpg" alt="billboard image" class="rounded-lg" />
</div>
<div class="order-1 lg:order-2 fadeInRight">
    <h2 class="text-3xl font-semibold text-gray-900 dark:text-gray-200">Authorized UNIVIEW-UAE Distributor</h2>
    <p class="text-md mt-4 dark:text-gray-300">At UNIVIEW-UAE, we are a leading UNV CCTV distributor in Dubai with over two decades of experience in the security industry. As one of the largest SIRA-approved CCTV companies in Dubai, we provide high-quality UNV CCTV cameras, including IP cameras, NVR systems, and other security solutions. <br><br>
    Our team is committed to helping businesses and individuals safeguard their properties with the latest and most advanced products from top global manufacturers like UNIVIEW. We also offer professional services for installation, configuration, and ongoing support.<br><br>
    Security is our priority, and we strive to provide exceptional customer service and tailored solutions to meet our clientsâ€™ needs. Contact us today to learn how we can help protect your property with reliable CCTV systems.
    </p>
    <div class="mt-8 grid w-full sm:inline-flex">
    <PrimaryCTA title="Contact Us" url=https://uniview-uae.ae/contact/ />
    </div>
        
    </div>
  </div>
 


  <div class="text-center mt-32 rounded-2xl mb-5">
